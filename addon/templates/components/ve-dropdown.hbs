<div class="dropdown-toggle disabled" data-toggle="dropdown">
  {{#if hasIcon }}
    <i class="fa {{ icon }}"></i>
  {{/if}}
  {{#if hasLabel }}
    <span class="dropdown-label">
      {{label}}
    </span>
  {{/if}}
  <span class="caret"></span>
</div>
<ul class="dropdown-menu" role="menu">
  {{yield}}
</ul>
